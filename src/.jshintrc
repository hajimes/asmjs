{
  "bitwise": true,
  "camelcase": true,
  "curly": true,
  "es3": false,
  "esnext": true,
  "forin": true,
  "freeze": true,
  "immed": true,
  "indent": 2,
  "latedef": "nofunc",
  "newcap": true,
  "noarg": true,
  "noempty": true,
  "plusplus": false,
  "quotmark": "single",
  "undef": true,
  "unused": true,
  "strict": "global",
  "trailing": true,
  "maxparams": 8,
  "maxdepth": 5,
  "maxlen": 240,
  "maxstatements": 1000,
  "globals": {
    "define": true,
    "require": true,
    "module": true,
    "max": true
  }
}
